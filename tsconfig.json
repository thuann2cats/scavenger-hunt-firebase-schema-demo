{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "outDir": "dist",
    "target": "ES2017",
    "module": "CommonJS",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ],
  
}
